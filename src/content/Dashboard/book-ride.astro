<!DOCTYPE html>
<html lang="en">
<head>
    
        <title>CarrGo - Ridesharing Taxi HTML Template</title>
        <meta charset="utf-8">
        <meta http-equiv="x-ua-compatible" content="ie=edge">
        <meta name="robots" content="follow, index"/>
        <meta name="description" content="CarrGo - Ridesharing Taxi HTML Template for building Ridesharing, Car-pooling and transportaion website">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="icon" type="image/png" sizes="16x16" href="/images/favicon-16x16.webp">
        <link rel="stylesheet" href="/plugins/bootstrap/css/bootstrap.min.css">
        <link rel="stylesheet" href="/plugins/animate/animate.css">
        <link rel="stylesheet" href="/plugins/swiper/swiper-bundle.min.css">
        <link rel="stylesheet" href="/icons/font-awesome/css/all.min.css">
        <link rel="stylesheet" href="/css/style.min.css">

</head>

  
        <!-- Ride Booking Partial -->
        <div class="ride-booking-section">
            <div class="container">
              <div class="row g-4 mb-4">
                <div class="col-lg-7">
                  <div class="booking-form-card">
                    <form action="#">
                      <!-- Destination Inputs -->
                      <div class="location-input">
                        <label for="inputFrom">From</label>
                        <div class="input-with-icon">
                          <i class="fas fa-map-marker-alt"></i>
                          <input type="text" name="frominputDestination" placeholder="Select Pickup" id="inputFrom" class="form-control">
                        </div>
                      </div>
              
                      <div class="location-input">
                        <label for="inputDestination">Where to?</label>
                        <div class="input-with-icon">
                          <i class="fas fa-map-marker-alt"></i>
                          <input type="text" name="destination" placeholder="Select Destination" id="inputDestination" class="form-control">
                        </div>
                      </div>
              
                 
                      <div style="display: flex;">
                 <!-- Vehicle Preference -->
                 <div class="col-md-6" style="padding: 2%;">
                  <label class="form-label fw-bold">Vehicle Type</label>
                  <div class="input-group">
                      <span class="input-group-text bg-light border-0">
                          <i class="fas fa-car-side text-muted"></i>
                      </span>
                      <select class="form-select border-0 bg-light" required>
                          <option value="">Select vehicle type</option>
                          <option>Economy Cars</option>
                          <option>Luxury Sedans</option>
                          <option>SUVs</option>
                          <option>Electric Vehicles</option>
                      </select>
                  </div>
              </div>
     <!-- Extra Options -->
     <div class="col-md-6" style="padding: 2%;">
      <label class="form-label fw-bold">Additional Options</label>
      <div class="input-group">
          <span class="input-group-text bg-light border-0">
              <i class="fas fa-plus-circle text-muted"></i>
          </span>
          <select class="form-select border-0 bg-light" multiple>
              <option>GPS Navigation</option>
              <option>Child Seat</option>
              <option>Additional Driver</option>
              <option>Full Insurance</option>
          </select>
      </div>
  </div>
</div>
  <!-- Real-time Availability Notice -->
  <div class="col-12">
      <div class="alert alert-info d-flex align-items-center mb-0" role="alert">
          <i class="fas fa-info-circle me-2"></i>
          <div>
              Showing ride availability. Travel allowed within local bounds of service.
          </div>
      </div>
  </div>
                   
               <!-- Submit Button -->
               <div class="col-12 text-end">
                <button type="submit" class="btn btn-theme fw-bold btn-lg px-5 ">
                    <i class="fas fa-calendar-check me-2"></i>
                    Check Availability
                </button>
            </div>
                     
                    </form>
                  </div>
                </div>
        
                <div class="col-lg-5">
                  <div class="ride-map-area">
                    <div class="mapouter">
                      <div class="gmap_canvas">
                        <iframe id="gmap_canvas" src="https://maps.google.com/maps?q=2880%20Balitmore,%20Maryland&t=&z=13&ie=UTF8&iwloc=&output=embed" frameborder="0" scrolling="no" marginheight="0" marginwidth="0"></iframe>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
    
   
  
</html>
